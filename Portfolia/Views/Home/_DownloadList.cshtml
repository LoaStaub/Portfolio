@model List<Portfolia.Models.Files>

@foreach (var file in Model)
{
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-primary">
                <div class="panel-heading">@file.FileName</div>
                <div class="panel-body">
                    <button class="btn btn-info" onclick="DownloadFile(@file.Id)">Download</button>
                </div>
            </div>
        </div>
    </div>
}
